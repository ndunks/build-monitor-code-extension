{
  "name": "esp-idf-build-command",
	"displayName": "ESP-IDF Build Command",
	"description": "Build flash and monitor from ESP-IDF Makefile style",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.x.x"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:esp-idf-build-command.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "esp-idf-build-command.helloWorld",
            "title": "Hello World"

		}]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.61.0",
        "@types/glob": "^7.1.4",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "eslint": "^7.32.0",
        "glob": "^7.1.7",
        "mocha": "^9.1.1",
        "typescript": "^4.4.3",
        "@vscode/test-electron": "^1.6.2"
    }

}
